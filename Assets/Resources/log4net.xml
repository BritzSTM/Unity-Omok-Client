<?xml version="1.0" encoding="utf-8"?>
<log4net>
	<appender name = "UnityDebugAppender" type ="WNet.Unity.LogAppender">
		<layout type="log4net.Layout.PatternLayout, log4net">	
			<conversionPattern value ="%m %c{1}:%L"/>
		</layout>
	</appender>
	<root>
		<level value="ALL"/>
		<appender-ref ref ="UnityDebugAppender"/>
	</root>
</log4net>
